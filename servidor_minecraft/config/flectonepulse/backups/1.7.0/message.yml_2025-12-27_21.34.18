  convert_legacy_color: true
  adventure_tags:
  - "HOVER"
  - "CLICK"
  - "COLOR"
  - "KEYBIND"
  - "TRANSLATABLE"
  - "TRANSLATABLE_FALLBACK"
  - "INSERTION"
  - "FONT"
  - "DECORATION"
  - "GRADIENT"
  - "RAINBOW"
  - "RESET"
  - "NEWLINE"
  - "TRANSITION"
  - "SELECTOR"
  - "SCORE"
  - "NBT"
  - "PRIDE"
  - "SHADOW_COLOR"
